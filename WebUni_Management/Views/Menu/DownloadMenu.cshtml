@model IEnumerable<WebUni_Management.Core.Models.Menu.MenuItemsViewModel>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Menu</title>
    <style>
    </style>
</head>
<body>
    <div class="menu-content">
        @foreach (var item in Model)
        {
            <div class="menu-item">
                <h2>@item.Name</h2>
                <p>Price: @item.Price</p>
                <p>Category: @item.Category</p>
            </div>
        }
    </div>
</body>
</html>